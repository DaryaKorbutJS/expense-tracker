{
    "name": "expense-tracker-backend",
    "version": "1.0.0",
    "main": "build/index.js",
    "scripts": {
        "start": "node build/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "test": "jest",
        "migrate": "prisma migrate dev",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@prisma/client": "^4.0.0",
        "dotenv": "^17.0.1",
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.7",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "husky": "^7.0.4",
        "jest": "^27.4.5",
        "prettier": "^2.4.1",
        "prisma": "^4.0.0",
        "ts-jest": "^27.1.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.4"
    }
}
